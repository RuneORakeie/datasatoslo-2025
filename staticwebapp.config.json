{
  "globalHeaders": {
    "Cache-Control": "no-cache, no-store, must-revalidate",
    "Pragma": "no-cache",
    "Expires": "0"
  },
  "routes": [
    {
      "route": "*.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Pragma": "no-cache",
        "Expires": "0"
      }
    },
    {
      "route": "*.css",
      "headers": {
        "Cache-Control": "public, max-age=2592000"
      }
    },
    {
      "route": "*.js",
      "headers": {
        "Cache-Control": "public, max-age=2592000"
      }
    },
    {
      "route": "*.png",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "route": "*.jpg",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "route": "*.jpeg",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "route": "*.gif",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "route": "*.svg",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "route": "*.ico",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ]
}