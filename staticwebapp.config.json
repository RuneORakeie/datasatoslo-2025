{
    "platform": {
      "apiRuntime": "node:20"
    },
    "build": {
      "env": {
        "HUGO_VERSION": "0.139.4",
        "HUGO_ENV": "production",
        "HUGO_EXTENDED": "true",
        "HUGO_ENABLEGITINFO": "true"
      }
    },
    "routes": [
      {
        "route": "/*",
        "serve": "/404.html",
        "statusCode": 404
      }
    ]
  }